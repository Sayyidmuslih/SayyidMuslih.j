#tipe data set
nama = {"Sayyid", 10, False, 5, "Muslih"}

#menampilkan set dengan perulangan
for i in nama:
 print(i)

nama =  list(nama)

i = 0
while i < len(nama):
 print(nama[i])
 i += 1

#mengupdate set 
nama = list(nama)

nama[1] = "Intan"
print(nama)

nama = set(nama)

#menghapus data set
nama.remove("Muslih")
print(nama)

nama.discard("Yanto")
print(nama)

nama.pop()
print(nama)

nama.clear()
print(nama)

#menambhakan set
nama.add("Intan")
print(nama)

nama.update(["Sayyid", "Amri"])
print(nama)
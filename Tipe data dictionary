#contoh dictionary
identitas = {
       'nama' : 'Sayyid Muslih',
       'usia' : 19,
     'status' : 'mahasiswa'
}

#menampilkan dictionary dengan perulangan
for i in identitas.items() :
  print(i)

for i in identitas :
  print(i)

for i in identitas.values() :
  print(i)
#mengupdate salah satu dictionary
identitas ['asal'] = 'Sendana'
print(identitas)

#menghapus dictionary
identitas.pop('asal')
print(identitas)

print(identitas.popitem())

#menambahkan dictionary